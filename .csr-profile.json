      "version": ".*",
      "ids": [
          "CVE-2018-15514",
          "CVE-2019-13139",
          "CVE-2019-13509",
          "CVE-2019-15752",
          "CVE-2019-16884",
          "CVE-2019-5736",
          "CVE-2020-14298",
          "CVE-2020-14300",
          "CVE-2018-10892",
          "CVE-2020-27534"
      ]
    }
  ]
}
